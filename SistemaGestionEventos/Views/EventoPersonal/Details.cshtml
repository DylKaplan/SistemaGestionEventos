@model SistemaGestionEventos.Models.EventoPersonal

@{
    ViewData["Title"] = "Detalle";
}

<h1>Detalle del Personal para el Evento</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Evento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Evento.Descripcion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Personal)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Personal.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Volver</a>
</div>
